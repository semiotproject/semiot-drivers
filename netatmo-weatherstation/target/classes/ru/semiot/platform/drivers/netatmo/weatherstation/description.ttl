@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssn: <http://purl.oclc.org/NET/ssnx/ssn#> .
@prefix mcht: <http://purl.org/NET/ssnext/machinetools#> .
@prefix ssncom: <http://purl.org/NET/ssnext/communication#> .
@prefix saref: <http://ontology.tno.nl/saref#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix dul: <http://www.loa-cnr.it/ontologies/DUL.owl#> .
@prefix qudt-quantity: <http://qudt.org/vocab/quantity#> .
@prefix qudt-unit: <http://qudt.org/vocab/unit#> .
@prefix qudt: <http://qudt.org/schema/qudt#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix proto: <http://w3id.org/semiot/ontologies/proto#> .
@prefix ws: <https://raw.githubusercontent.com/semiotproject/semiot-platform/master/device-proxy-service-drivers/netatmo-weatherstation/src/main/resources/ru/semiot/platform/drivers/netatmo/weatherstation/prototype.ttl#> .

<${ru.semiot.platform.systems_uri_prefix}/${ru.semiot.platform.device.id}> a ssn:System, proto:Individual ;
    rdfs:label "Netatmo Weather Station (Outdoor module) / ${ru.semiot.platform.device.id}"@en ;
    proto:hasPrototype ws:NetatmoWeatherStationOutdoorModule ;
    ssn:hasSubSystem <${ru.semiot.platform.systems_uri_prefix}/${ru.semiot.platform.device.id}/${ru.semiot.platform.subsystem_path}/${ru.semiot.platform.device.id}-temperature> ;
    ssn:hasSubSystem <${ru.semiot.platform.systems_uri_prefix}/${ru.semiot.platform.device.id}/${ru.semiot.platform.subsystem_path}/${ru.semiot.platform.device.id}-humidity> ;
    dcterms:identifier "${ru.semiot.platform.device.id}"^^xsd:string ;
    geo:location [
	    a geo:Point ;
	    geo:lat "${ru.semiot.drivers.netatmo.latitude}"^^xsd:string ;
        geo:long "${ru.semiot.drivers.netatmo.langitude}"^^xsd:string ;
        geo:alt "${ru.semiot.drivers.netatmo.altitude}"^^xsd:string ;
    ] ;
    .

<${ru.semiot.platform.systems_uri_prefix}/${ru.semiot.platform.device.id}/${ru.semiot.platform.subsystem_path}/${ru.semiot.platform.device.id}-temperature>
    a ssn:SensingDevice, proto:Individual ;
    proto:hasPrototype ws:NetatmoWeatherStationOutdoorModule-TemperatureSensor ;
    dcterms:identifier "${ru.semiot.platform.device.id}-temperature"^^xsd:string ;
    .

<${ru.semiot.platform.systems_uri_prefix}/${ru.semiot.platform.device.id}/${ru.semiot.platform.subsystem_path}/${ru.semiot.platform.device.id}-humidity>
    a ssn:SensingDevice, proto:Individual ;
    proto:hasPrototype ws:NetatmoWeatherStationOutdoorModule-HumiditySensor ;
    dcterms:identifier "${ru.semiot.platform.device.id}-humidity"^^xsd:string ;
    .