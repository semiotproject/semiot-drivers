    geo:location [
        a schema:Place ;
        schema:branchCode "{{ru.semiot.platform.drivers.semiot_th_s.room}}"^^xsd:integer ;
        rdfs:label "Комната {{ru.semiot.platform.drivers.semiot_th_s.room}}"@ru ;
